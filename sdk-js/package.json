{
  "name": "@4runr/gateway",
  "version": "1.0.0",
  "description": "Official SDK for 4Runr Gateway - Secure API proxy with built-in authentication, policy enforcement, and resilience",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "jest",
    "lint": "eslint src --ext .ts",
    "clean": "rm -rf dist",
    "prepublishOnly": "npm run clean && npm run build"
  },
  "keywords": [
    "4runr",
    "gateway",
    "api",
    "proxy",
    "security",
    "authentication",
    "policy",
    "resilience"
  ],
  "author": "4Runr Team",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "node-fetch": "^3.3.2"
  },
  "devDependencies": {
    "@types/node": "^20.19.9",
    "@types/node-fetch": "^2.6.13",
    "typescript": "^5.0.0",
    "tsup": "^8.0.0",
    "jest": "^29.0.0",
    "@types/jest": "^29.0.0",
    "eslint": "^8.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/4runr/gateway-sdk-js.git"
  },
  "bugs": {
    "url": "https://github.com/4runr/gateway-sdk-js/issues"
  },
  "homepage": "https://github.com/4runr/gateway-sdk-js#readme"
}
